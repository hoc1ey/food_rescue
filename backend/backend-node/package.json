{
  "name": "backend",
  "version": "1.0.0",
  "description": "Food Rescue Live-Board Backend (Node.js, Express, TypeScript, Socket.io)",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc && cpy \"src/generated/prisma/**/*\" dist/generated/prisma --parents",
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "seed": "tsx src/seed/seed.ts",
    "watch": "tsc -w & tsx watch dist/app.js"
  },
  "keywords": [
    "express",
    "typescript",
    "socketio",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.6.1",
    "express": "^4.22.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "qs": "^6.14.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "cpy-cli": "^6.0.0",
    "nodemon": "^3.1.11",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
